{
    "name": "pepipost/pepipost-lib",
    "description": "Convenient way to send emails from laravel through pepipost api",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pepipost\\PepipostLib\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pepipost\\PepipostLib\\PepipostServiceProvider"
            ],
            "aliases": {
                "Pepipostmailer": "Pepipost\\PepipostLib\\Facades\\Pepipostmailer"
            }
        }
    },
    "authors": [
        {
            "name": "Hiteshpandey",
            "email": "hiteshpandey00@gmail.com"
        }
    ],
    "require": {
        "apimatic/unirest-php": "^3.0",
        "apimatic/jsonmapper": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    }
}
